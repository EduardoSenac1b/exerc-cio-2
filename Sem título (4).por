programa {
  funcao inicio() {
    inteiro mais
    inteiro menos
    inteiro vezes
    real divisao
    inteiro n1
    inteiro n2
    escreva("insira dois numeros: ")
    leia(n1 , n2)
    mais = n1 + n2
    menos = n1 - n2
    vezes = n1 * n2
    divisao = n1 / n2
    escreva("mais: ", mais ,"\n")
    escreva("menos: ", menos , "\n")
    escreva("vezes:", vezes , "\n")
    escreva("divisão: ", divisao , "\n")
  }
}
